{
  "name": "gchatapp",
  "version": "1.0.0",
  "description": "Chat App",
  "main": "index.js",
  "scripts": {
    "start":"node build/app.js",
    "heroku-postbuild":"env-cmd ./env/prod.env babel src --out-dir build --copy-files",
    "dev": "env-cmd ./env/dev.env nodemon --exec babel-node src/app.js",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build"
  },
  "author": "George Mickael",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.19",
    "mongoose-findorcreate": "^3.0.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "socket.io": "^2.2.0",
    "env-cmd": "^8.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "nodemon": "^1.18.10"
  }
}
